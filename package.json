{
  "name": "ysba-standings",
  "version": "2.0.0",
  "description": "YSBA 9U Select Baseball Standings with GitHub Gist Backup",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node scripts/update-cache-version.js"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "node_modules/",
      ".cache/",
      "*.log"
    ],
    "watch": [
      "server.js",
      "scraper.js",
      "config.js"
    ],
    "delay": 2000
  },
  "keywords": [
    "baseball",
    "standings",
    "ysba",
    "scraper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.8.2",
    "simplycountdown.js": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
